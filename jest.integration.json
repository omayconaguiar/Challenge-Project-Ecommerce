{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "test/integration",
  "testMatch": ["**/*.spec.ts", "**/*.int-spec.ts"],
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "collectCoverageFrom": [
    "**/*.(t|j)s"
  ],
  "coverageDirectory": "../coverage",
  "testEnvironment": "node"
}
